    <script type="text/javascript">

    function ModalUpdProgInitialize(sender, args) {var upd = $find("ctl00_ctl00_AjaxProgress"); 
    upd._pageRequestManager.remove_endRequest(upd._endRequestHandlerDelegate);

    upd._endRequestHandlerDelegate = Function.createDelegate(upd, ModalUpdProgEndRequest);

    upd._startDelegate = Function.createDelegate(upd, ModalUpdProgStartRequest);

    upd._pageRequestManager.add_endRequest(upd._endRequestHandlerDelegate);

    }

    function ModalUpdProgStartRequest() { 
    if (this._pageRequestManager.get_isInAsyncPostBack()) {

        $find("ctl00_ctl00_ModalExtender1").show();
        el = document.getElementById("progressAccessible");
        move_focus(el);
    }

    this._timerCookie = null; 
    }

    var el;

    function ModalUpdProgEndRequest(sender, arg) { 
    $find("ctl00_ctl00_ModalExtender1").hide();

    if (this._timerCookie) { 
    window.clearTimeout(this._timerCookie);

    this._timerCookie = null; 
    }

    }

