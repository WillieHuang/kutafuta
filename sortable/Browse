 <script type="text/javascript">
	function onProductAddedToBasket(addEventXpos,addEventYpos)
	{
	    //alert("onProductAddedToBasketInInlineBasket : " + addEventYpos + " : " + addEventXpos);
	    //alert("appVersion : " + navigator.appVersion + "\nappName : " + navigator.appName + "\nuserAgent : " + navigator.userAgent);
	    var message = $get("ItemAddedMessage");
	    var yScroll = 0;
	    var ie6 = false;
	    var ff2 = false;
	    
	    if (navigator.appVersion.indexOf("MSIE 6.0") > 0) {
	        ie6 = true;
	    }

	    if (ie6 && document.documentElement.scrollTop) {
	        yScroll = document.documentElement.scrollTop;
	    }
	    
		if (message != null) {

		    message.style.display = "block";
		    if (ie6) {
		        message.style.position = "absolute";
		    }	
		    message.style.left = addEventXpos + 5 + "px";
		    message.style.top = addEventYpos + yScroll - 20 + "px";
		    setTimeout(hideItemAddedMessage, 2000);
		}
	}
	function hideItemAddedMessage() {
	    var message = $get("ItemAddedMessage");
	    if (message != null) {
	        $get("ItemAddedMessage").style.display = "none";
	    }
	}
  </script>
