//    		accessibility.appendChild(newContainer);
//    	}

//    }
    
    //Sys.Application.add_init(PrepareAccessibilityRequirements);
    Sys.Application.add_load(ModalUpdProgInitialize);
    
    function scrollSubMenuUNav(){
    	
	    if(document.getElementById("WebHelpUNav_SubMenu").style.display == "none")
	    {
		    document.getElementById("WebHelpUNav_SubMenu").style.display = "block";
		    document.getElementById("WebHelpUNav_Arrow").className = "WebHelpUNav_ArrowUp";
	    }
	    else{
		    document.getElementById("WebHelpUNav_SubMenu").style.display = "none";
		    document.getElementById("WebHelpUNav_Arrow").className = "WebHelpUNav_ArrowDown";
	    }
    }

    </script>
