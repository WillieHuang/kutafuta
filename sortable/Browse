    <script language="javascript">
        function changeCatalog() {
            var element1 = $get("ctl00_ctl00_InlinePopupContainer_InlinePopup_DummyLink");
            element1.ModalPopupBehavior.show();
        }

        function ChangeCurrent() {
            var currentCatalog = document.getElementById("currentCatalog");
            var nextCatalog = document.getElementById("nextCatalog");

            if (nextCatalog.checked) {
                var catalogChangedLink = $get('ctl00_ctl00_MainContent_MainContent_CatalogChanged');
                nextCatalog = $get('ctl00_ctl00_MainContent_MainContent_NextCatalog');
                if (("" + catalogChangedLink.href).indexOf('?') == -1) {
                    catalogChangedLink.href += "?catalogName=" + nextCatalog.value;
                }
                else {
                    catalogChangedLink.href += "&catalogName=" + nextCatalog.value;
                }

                if (catalogChangedLink.click) {
                    catalogChangedLink.click();
                }
                else {
                    location.href = catalogChangedLink.href;
                }
            }
        }

        function AddToQuickList(productId, quantity) {
            var frame = $get("addToQuickListFrame");
            var baseUrl = 'http://magasin.iga.net/Member/AddToQuickList.aspx';
            var url = baseUrl + "?ProductId=" + productId + "&Quantity=" + quantity;
            frame.src = url;
            var element2 = $get("ctl00_ctl00_InlinePopupContainer_InlinePopup_DummyLink2");
            element2.ModalPopupBehavior.show();

            var buddy = $get("accessiblePopupAddQuickList");
            buddy.focus();
        }

        function CloseAddToQuickListModalDialog() {
            var frame = $get("addToQuickListFrame");
            frame.src = "about:blank";

            var element2 = $get("ctl00_ctl00_InlinePopupContainer_InlinePopup_DummyLink2");
            element2.ModalPopupBehavior.hide();

            var buddy = $get("AccessibleSkipNavigationLink");
            buddy.focus();
        }

        function showProductImage(pimg, pname) {
            var smallProductImage = document.getElementById(pimg);
            var productImageIframe = document.getElementById('productImageIframe');
            if (smallProductImage != null && smallProductImage.getAttribute('highres') != '') {
                if (productImageIframe != null) {
                    var productUrl = smallProductImage.getAttribute('highres');
                    var theUrl = '/CatalogPages/ProductImagePage.aspx?pimg=' + productUrl + '&pname=' + pname;
                    productImageIframe.src = theUrl;
                    productImageIframe.title = pname;
                    var element3 = $get("ctl00_ctl00_InlinePopupContainer_InlinePopup_DummyLink3");
                    element3.ModalPopupBehavior.show();
                }
            }
        }

        function closeButtonReset() {
            var productImageIframe = document.getElementById('productImageIframe');
            productImageIframe.src = '';
        }
    </script>



