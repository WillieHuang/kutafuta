<script language="javascript">
    function showIFrame()
    {
        var iframe = $get('searchListFakeFrameId');
        if (navigator.userAgent.indexOf("MSIE 6") > -1)
        {
            iframe.style.display = 'block';
        }
    }
    
    function hideIFrame()
    {
        if (navigator.userAgent.indexOf("MSIE 6") > -1)
        {
        var iframe = $get('searchListFakeFrameId');
        iframe.style.display = 'none';
        }
    }
</script>

<label for="ctl00_ctl00_ProductSearch1_searchBox" id="ctl00_ctl00_ProductSearch1_SearchBoxLabel">Trouver un produit</label>
                            
<div class="inputWrapper">
    <input name="ctl00$ctl00$ProductSearch1$searchBox" type="text" id="ctl00_ctl00_ProductSearch1_searchBox" onkeydown="return searchBox_KeyDown(event)" />
    
    
    
</div>
<script type="text/javascript">
function ExecInPageSearch()
{
	var inputBox = $get("ctl00_ctl00_ProductSearch1_searchBox");
	if(inputBox.value.length>0)
	{
		document.location = siteRoot + "Search/BasicSearch.aspx?Search=" + $get("ctl00_ctl00_ProductSearch1_searchBox").value ;
	}
	
}
function searchBox_KeyDown(event)
{
		
	if(event.keyCode == 13 || event.keyCode == 10)
	{
		
		ExecInPageSearch();
		event.cancelBubble = true;
		if(event.stopPropagation)event.stopPropagation();
		event.returnValue = false;
		return false;
		//alert(event.keyCode);
	}
	return true;
}
</script>


                          
